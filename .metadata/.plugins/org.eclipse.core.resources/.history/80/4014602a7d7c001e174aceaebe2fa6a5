package modelo;

public class Reference {

	private char row;
	private int colum;

	public Reference(char row, int seat) {
		super();
		this.row = row;
		this.colum = seat;
	}

	public char getRow() {
		return row;
	}

	public void setRow(char row) {
		this.row = row;
	}

	public int getColum() {
		return colum;
	}

	public void setColum(int colum) {
		this.colum = colum;
	}



}
